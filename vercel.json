{
  "rewrites": [
    {
      "source": "/((?!_next/|favicon.ico|images/|css/|docs/).*)",
      "has": [{ "type": "host", "value": "locker.steakhouse.finance" }],
      "destination": "/locker/$1"
    },
    {
      "source": "/((?!_next/|favicon.ico|images/|css/|docs/).*)",
      "has": [{ "type": "host", "value": "explore.steakhouse.finance" }],
      "destination": "/explore/$1"
    },
    {
      "source": "/((?!_next/|favicon.ico|images/|css/|docs/).*)",
      "has": [{ "type": "host", "value": "curve.steakhouse.finance" }],
      "destination": "/trading-chart/$1"
    }
  ]
}
